  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start -- -p ${PORT:-3000}",
    "lint": "tslint -c tslint.js -p tsconfig.json",
    "prettier": "prettier --write './pages/**/*.{js,jsx,ts,tsx}'",
    "prettier:check": "prettier --list-different './pages/**/*.{js,jsx,ts,tsx}'",
    "build": "next build",
    "test": "echo 'Todo'",
    "data": "npx ts-node --project scripts/tsconfig.json scripts/data",
    "assets": "npx ts-node --project scripts/tsconfig.json scripts/assets",
    "sync": "yarn data && yarn assets",
    "push": "npx ts-node --project scripts/tsconfig.json scripts/updater",
    "sync:push": "yarn sync && yarn push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "next": "^9.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.15",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.11",
    "cheerio": "^1.0.0-rc.3",
    "got": "^10.2.2",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "simple-git": "^1.130.0",
    "throat": "^5.0.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
